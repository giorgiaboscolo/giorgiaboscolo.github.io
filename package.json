{
    "name": "@giorgiaboscolo/giorgiaboscolo.github.io",
    "version": "1.0.0",
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/giorgiaboscolo"
    },
    "private": false,
    "scripts": {
        "dev": "vite",
        "preview": "vite preview --port 5050",
        "build": "vue-tsc --noEmit && vite build",
        "typecheck": "vue-tsc --noEmit",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
    },
    "files": [
        "src/**",
        "tailwind.config.js"
    ],
    "dependencies": {
        "@headlessui/vue": "^1.7.12",
        "@heroicons/vue": "^2.0.13",
        "@vueuse/components": "^9.10.0",
        "@vueuse/core": "^9.10.0",
        "animate.css": "^4.1.1",
        "core-js": "^3.27.1",
        "date-fns": "^2.29.3",
        "lodash": "^4.17.21",
        "pinia": "^2.0.28",
        "sass": "^1.57.1",
        "uuid": "^9.0.0",
        "v-viewer": "^3.0.11",
        "vee-validate": "^4.7.3",
        "vue": "^3.2.45",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.1.6",
        "vue-tippy": "^6.0.0"
    },
    "devDependencies": {
        "@intlify/vite-plugin-vue-i18n": "^3.4.0",
        "@rushstack/eslint-patch": "^1.2.0",
        "@types/node": "^16.18.23",
        "@types/youtube": "^0.0.47",
        "@types/jsdom": "^16.2.14",
        "@vitejs/plugin-vue": "^2.3.3",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.2",
        "@vue/tsconfig": "^0.1.3",
        "autoprefixer": "^10.4.13",
        "eslint": "^8.31.0",
        "eslint-plugin-vue": "^9.8.0",
        "postcss": "^8.4.21",
        "prettier": "^2.8.2",
        "prettier-plugin-tailwindcss": "^0.2.1",
        "tailwindcss": "^3.3.1",
        "typescript": "~4.9.4",
        "vite": "^2.9.12",
        "vue-tsc": "^1.0.24"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended",
            "@vue/typescript/recommended"
        ],
        "parserOptions": {
            "ecmaVersion": 2020
        },
        "rules": {
            "no-console": "off",
            "no-inferrable-types": 0,
            "indent": 0,
            "interface-name": 0,
            "max-classes-per-file": 0,
            "max-line-length": 0,
            "no-consecutive-blank-lines": 0,
            "ordered-imports": 0,
            "object-literal-sort-keys": 0,
            "prefer-for-of": 0,
            "quotemark": 0,
            "trailing-comma": 0,
            "variable-name": 0,
            "no-mixed-spaces-and-tabs": 0,
            "camel-case": "off"
        }
    }
}
